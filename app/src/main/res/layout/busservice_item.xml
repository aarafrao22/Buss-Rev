<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="4dp"
    android:background="@drawable/border"
    android:padding="2dp">

    <ImageView
        android:id="@+id/roadimages"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/ic_baseline_directions_24" />

    <TextView
        android:id="@+id/directionname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/roadimages"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="0dp"
        android:text="0"
        android:textColor="#fff" />

    <TextView
        android:id="@+id/ServiceNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="4dp"
        android:layout_toRightOf="@+id/roadimages"
        android:text="Service COde"
        android:textColor="#fff"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/Category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ServiceNo"
        android:layout_marginStart="30dp"
        android:layout_marginTop="-1dp"
        android:layout_toEndOf="@+id/roadimages"
        android:text="Victoria St"
        android:textColor="#fff" />


    <ImageView
        android:id="@+id/AM_Peak_Freqlabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/ServiceNo"
        android:src="@drawable/ic_baseline_access_time_filled_24"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/AM_Peak_Freqvalue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/AM_Peak_Freqlabel"
        android:layout_marginStart="14dp"
        android:layout_marginTop="8dp"
        android:layout_toRightOf="@+id/ServiceNo"
        android:text="5-08"
        android:textColor="#fff"
        android:textSize="25px" />

    <ImageView
        android:id="@+id/AM_Offpeak_Freqlabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/AM_Peak_Freqlabel"
        android:src="@drawable/ic_baseline_access_time_filled_24"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/AM_Offpeak_Freqvalue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/AM_Peak_Freqlabel"
        android:layout_marginStart="14dp"
        android:layout_marginTop="8dp"
        android:layout_toRightOf="@+id/AM_Peak_Freqlabel"
        android:text="5-08"
        android:textColor="#fff"
        android:textSize="25px" />

    <ImageView
        android:id="@+id/PM_Peak_Freqlabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/AM_Offpeak_Freqlabel"
        android:src="@drawable/ic_baseline_access_time_filled_24"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/PM_Peak_Freqvalue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/AM_Peak_Freqlabel"
        android:layout_marginStart="14dp"
        android:layout_marginTop="8dp"
        android:layout_toRightOf="@+id/AM_Offpeak_Freqlabel"
        android:text="5-08"
        android:textColor="#fff"
        android:textSize="25px" />

    <ImageView
        android:id="@+id/PM_Offpeak_Freqlabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/PM_Peak_Freqlabel"
        android:src="@drawable/ic_baseline_access_time_filled_24"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/PM_Offpeak_Freqvalue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/AM_Peak_Freqlabel"

        android:layout_marginStart="14dp"
        android:layout_marginTop="8dp"

        android:layout_toRightOf="@+id/PM_Peak_Freqlabel"
        android:text="5-08"
        android:textColor="#fff"
        android:textSize="25px" />


</RelativeLayout>